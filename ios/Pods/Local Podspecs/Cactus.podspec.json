{
  "name": "Cactus",
  "version": "1.2.0",
  "summary": "Run AI models locally on mobile devices",
  "homepage": "https://github.com/cactus-compute/cactus-react-native#readme",
  "license": "MIT",
  "authors": "cactus-compute <founders@cactus-compute.com> (https://cactuscompute.com)",
  "platforms": {
    "ios": "15.1"
  },
  "source": {
    "git": "https://github.com/cactus-compute/cactus-react-native.git",
    "tag": "1.2.0"
  },
  "source_files": [
    "ios/**/*.{swift}",
    "ios/**/*.{m,mm}",
    "cpp/**/*.{h,hpp,cpp}",
    "nitrogen/generated/shared/**/*.{h,hpp,c,cpp,swift}",
    "nitrogen/generated/ios/**/*.{h,hpp,c,cpp,mm,swift}"
  ],
  "vendored_frameworks": [
    "ios/cactus.xcframework",
    "ios/cactus_util.xcframework"
  ],
  "dependencies": {
    "React-jsi": [],
    "React-callinvoker": [],
    "ZIPFoundation": [
      "~> 0.9"
    ],
    "NitroModules": [],
    "React-Core": [],
    "React-RCTFabric": [],
    "ReactCodegen": [],
    "RCTRequired": [],
    "RCTTypeSafety": [],
    "ReactCommon/turbomodule/bridging": [],
    "ReactCommon/turbomodule/core": [],
    "React-NativeModulesApple": [],
    "Yoga": [],
    "React-Fabric": [],
    "React-graphics": [],
    "React-utils": [],
    "React-featureflags": [],
    "React-debug": [],
    "React-ImageManager": [],
    "React-rendererdebug": [],
    "React-renderercss": [],
    "hermes-engine": [],
    "glog": [],
    "boost": [],
    "DoubleConversion": [],
    "fast_float": [],
    "fmt": [],
    "RCT-Folly": [],
    "SocketRocket": [],
    "RCT-Folly/Fabric": []
  },
  "public_header_files": [
    "nitrogen/generated/shared/**/*.{h,hpp}",
    "nitrogen/generated/ios/Cactus-Swift-Cxx-Bridge.hpp"
  ],
  "private_header_files": [
    "nitrogen/generated/ios/c++/**/*.{h,hpp}",
    "nitrogen/generated/shared/**/views/**/*"
  ],
  "pod_target_xcconfig": {
    "CLANG_CXX_LANGUAGE_STANDARD": "c++20",
    "SWIFT_OBJC_INTEROP_MODE": "objcxx",
    "DEFINES_MODULE": "YES",
    "HEADER_SEARCH_PATHS": [
      "\"$(PODS_ROOT)/Headers/Private/Yoga\"",
      "$(PODS_ROOT)/glog",
      "$(PODS_ROOT)/boost",
      "$(PODS_ROOT)/DoubleConversion",
      "$(PODS_ROOT)/fast_float/include",
      "$(PODS_ROOT)/fmt/include",
      "$(PODS_ROOT)/SocketRocket",
      "$(PODS_ROOT)/RCT-Folly"
    ],
    "OTHER_CPLUSPLUSFLAGS": "$(inherited) -DRCT_NEW_ARCH_ENABLED=1 ",
    "OTHER_SWIFT_FLAGS": "$(inherited) -DRCT_NEW_ARCH_ENABLED"
  },
  "compiler_flags": "-DRCT_NEW_ARCH_ENABLED=1"
}
